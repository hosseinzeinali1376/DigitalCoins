<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.digitalcoins">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".di.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DigitalCoins">
        <activity android:name=".view.holderactivity.HolderActivity"></activity>
        <activity android:name=".view.buysellactivity.Buy_Sell_Activity" />
        <activity android:name=".view.splash.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.mainactivity.MainActivity" />
        <activity android:name=".view.portfo.Portfo" />
    </application>

</manifest>